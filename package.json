{
  "name": "nextjs-preact-demo",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn run watch:css & next",
    "build": "yarn run build:css & next build && next export -o build",
    "start": "yarn run watch:css & NODE_ENV=production next start",
    "format": "yarn test:lint --fix",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css --watch",
    "test:types": "tsc",
    "test:lint": "tslint 'src/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@prefresh/next": "^1.0.0",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "react-refresh": "^0.8.3"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "next": "^9.5.4",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "postcss-cli": "^7.1.2",
    "preact": "^10.4.8",
    "preact-render-to-string": "^5.1.10",
    "prettier": "^2.1.1",
    "react": "github:preact-compat/react#1.0.0",
    "react-dom": "github:preact-compat/react-dom#1.0.0",
    "react-multi-carousel": "^2.5.5",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.0.1",
    "react-typist": "^2.0.5",
    "tailwindcss": "^1.7.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.2",
    "webp-loader": "^0.6.0"
  },
  "license": "MIT"
}
